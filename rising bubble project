ArrayList <Bubble> myBubble;
Bubble myBub;
void setup() {
  frameRate(5);
  size(1000, 700);
  myBubble = new ArrayList<Bubble> ();
  myBub = new Bubble();
  myBubble.add(myBub);
}

void draw() {
  background(255);
  for (int i = 0; i < 100; i++) {
    myBub = new Bubble();
    myBubble.add(myBub);
    myBub.show();
  }
}
